
@{
    ViewBag.Title = "डिजीटल क्लासरूम";
    Layout = "~/Views/Shared/_LayoutWebsite.cshtml";
}
<div class="full-widht-bread">
    <div class="container">
        <div class="row">
            <div class="col-12 col-md-8">
                <nav aria-label="breadcrumb-sm">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item">@Html.ActionLink("मुखपृष्ठ", "Index", "Home")</li>
                        <li class="breadcrumb-item active" aria-current="page">सुविधा</li>
                        <li class="breadcrumb-item active" aria-current="page">@ViewBag.Title</li>

                    </ol>
                </nav>
            </div>
        </div>
    </div>
</div>

<div class="full-widht-pagedata">
    <div class="container container-bg padding-bottom-15 padding-top-15">
        <div class="row">

            <div class="col-lg-8 col-xm-12 col-md-8 col-sm-12 card p-0">
                <div class="card-header font-weight-bold">
                    <strong>@ViewBag.Title</strong>
                </div>
                    <img src="~/img/Departments/digital_lab.png" class="title-image" />

                    
                    <div class="col">
                       
                        <p class="text-justify">
                            आपल्या विद्यालयात बालवाडी पर्यंतच्या विद्यार्थ्यांना इंटरनेट सुविधेसाह अद्यावत अशी डिजीटल रूमची सोय करण्यात आली आहे.
                            त्याद्वारे विद्यार्थ्यानी सर्व विषय सोप्या भाषेत चित्रफितीसह अध्ययन करणे सोपे जाते.
                        </p>


                    </div>
                </div>
                <div class="col-lg-4 col-xm-12 col-md-4 col-sm-12">
                    @Html.Partial("_nofitications")
                </div>
            </div>

        </div>
</div>



