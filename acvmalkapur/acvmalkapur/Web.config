  <log4net>
    <appender name="Console" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <!-- Pattern to output the caller's file name and line number -->
        <conversionPattern value="%5level [%thread] (%file:%line) - %message%newline" />
      </layout>
    </appender>
    <appender name="DailyAppenderWithPrefixExtension" type="Application.Utilities.DailyAppender,Application.Utilities">
      <File type="log4net.Util.PatternString" value="C:\Logs\Log4Net" />
      <AppendToFile value="true" />
      <param name="RollingStyle" value="Date" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <Extension value=".log" />
      <Prefix value="Webiste" />
      <param name="maxSizeRollBackups" value="30" />
      <param name="maximumFileSize" value="10MB" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date  [%-5level]  [Thread_%thread]  [%logger]  %message%newline" />
      </layout>
    </appender>

    <root>
      <level value="DEBUG" />
      <appender-ref ref="Console" />
      <appender-ref ref="DailyAppenderWithPrefixExtension" />
    </root>
  </log4net>
