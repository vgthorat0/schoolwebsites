
@{
    ViewBag.Title = "Library";
    Layout = "~/Views/Shared/_LayoutWebsite.cshtml";
}
<div class="full-widht-bread">
    <div class="container">
        <div class="row">
            <div class="col-12 col-md-8">
                <nav aria-label="breadcrumb-sm">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item">@Html.ActionLink("Home", "Index", "Home")</li>
                        <li class="breadcrumb-item active" aria-current="page">Facilities</li>
                        <li class="breadcrumb-item active" aria-current="page">@ViewBag.Title</li>

                    </ol>
                </nav>
            </div>
        </div>
    </div>
</div>

<div class="full-widht-pagedata">
    <div class="container container-bg padding-bottom-15 padding-top-15">
        <div class="row">

            <div class="col-lg-8 col-xm-12 col-md-8 col-sm-12 card p-0">
                <div class="card-header font-weight-bold">
                    <strong>@ViewBag.Title</strong>
                </div>
                <img src="~/img/departments/library.png" class="title-image" />

                @*<div class="col title-para">&nbsp; <br /> <strong>ग्रंथालय व पुस्तकपेढी</strong> </div>*@
                <div class="col">
                    <p class="text-justify">
                        <br />
                        In our institute, library consist of more than 1000 books of different sections(KG, PRIMARY, SECONDARY AND HIGHER SECONDARY). Library is free for all students, monthly magzines and daily different newspapers keep students updated with current affairs.
                    </p>
                </div>
            </div>
            <div class="col-lg-4 col-xm-12 col-md-4 col-sm-12">
                @Html.Partial("_nofitications")
            </div>
        </div>

    </div>
</div>



