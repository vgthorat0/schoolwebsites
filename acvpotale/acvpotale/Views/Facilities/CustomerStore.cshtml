
@{
    ViewBag.Title = "ग्राहक भांडार";
    Layout = "~/Views/Shared/_LayoutWebsite.cshtml";
}
<div class="full-widht-bread">
    <div class="container">
        <div class="row">
            <div class="col-12 col-md-8">
                <nav aria-label="breadcrumb-sm">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item">@Html.ActionLink("मुखपृष्ठ", "Index", "Home")</li>
                        <li class="breadcrumb-item active" aria-current="page">सुविधा</li>
                        <li class="breadcrumb-item active" aria-current="page">@ViewBag.Title</li>

                    </ol>
                </nav>
            </div>
        </div>
    </div>
</div>

<div class="full-widht-pagedata">
    <div class="container container-bg padding-bottom-15 padding-top-15">
        <div class="row">

            <div class="col-lg-8 col-xm-12 col-md-8 col-sm-12 card p-0">
                <div class="card-header font-weight-bold">
                    <strong>@ViewBag.Title</strong>
                </div>
                <img src="~/img/depatments/customer_store.PNG" class="title-image" />

              
                <div class="col">
                    <p class="text-justify"><br />

                        विद्यार्थ्यांना विद्यार्थ्यांसाठी विद्यार्थ्यांचा मदतीने ग्राहक भांडार १९९३ साली “जीवनज्योती सह.विद्यार्थी ग्राहक संस्था”’ या नावाने नवीन संस्था “ना नफा ना तोटा” या तत्वावर सुरू केली. नोंदनिकृत संस्थेचे २१२ सभासद असून संचालक मंडळाचा पारदर्शी कारभारमुळे संस्था प्रगतिपथावर आहे. आजअखेर ऑडिट वर्ग “A” मिळाला आहे.येथे विद्यार्थ्यांसाठी सर्व शैक्षणिक साहित्य अल्प दरात उपलब्ध करून दिले जाते.
                    </p>
                </div>
            </div>
            <div class="col-lg-4 col-xm-12 col-md-4 col-sm-12">
                @Html.Partial("_nofitications")
            </div>
        </div>

    </div>
</div>



